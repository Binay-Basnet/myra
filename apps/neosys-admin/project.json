{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/neosys-admin",
  "projectType": "application",
  "targets": {
    "tsc": {
      "executor": "@webpro/nx-tsc:tsc",
      "options": {
        "tsConfig": ["tsconfig.json"]
      }
    },
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/neosys-admin",
        "outputPath": "dist/apps/neosys-admin"
      },
      "configurations": {
        "production": {
          "outputPath": "apps/neosys-admin/dist"
        },
        "development": {
          "outputPath": "apps/neosys-admin"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "neosys-admin:build",
        "dev": true,
        "port": 4600
      },
      "configurations": {
        "production": {
          "buildTarget": "neosys-admin:build:production",
          "dev": false
        },
        "development": {
          "buildTarget": "neosys-admin:build:development",
          "dev": true
        }
      },
      "defaultConfiguration": "development"
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "neosys-admin:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/neosys-admin"],
      "options": {
        "jestConfig": "apps/neosys-admin/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/neosys-admin/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
