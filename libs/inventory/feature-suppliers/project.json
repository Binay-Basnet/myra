{
  "sourceRoot": "libs/inventory/feature-vendors/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "tsc": {
      "executor": "@webpro/nx-tsc:tsc",
      "options": {
        "tsConfig": ["tsconfig.json"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/inventory/feature-vendors/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/inventory/feature-vendors"],
      "options": {
        "jestConfig": "libs/inventory/feature-vendors/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
