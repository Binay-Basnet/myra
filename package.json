  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "start:dhadda-migration": "concurrently \"nx serve dhadda-migration\"",
    "start:myra": "concurrently \"nx serve myra\" \"yarn graphql-cbs:codegen\"",
    "start:migration": "concurrently \"nx serve migration\" \"yarn graphql-migration:codegen\"",
    "start:neosys-admin": "concurrently \"nx serve neosys-admin\" \"yarn graphql-neosys:codegen\"",
    "start:ebanking": "concurrently \"nx serve ebanking\" \"yarn graphql-ebank:codegen\"",
    "start:all": "concurrently \"yarn start:myra\" \"yarn start:neosys-admin\" \"yarn start:ebanking\"",
    "start:employee-portal": "concurrently \"nx serve employee-portal\" \"yarn graphql-emp:codegen\"",
    "start:csv-viewer": "nx serve csv-viewer",
    "storybook": "nx run @myra-ui:storybook",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens ./libs/@myra/theme/src/lib/theme/index.ts",
    "graphql-cbs:codegen": "graphql-codegen -r dotenv/config --config libs/cbs/data-access/codegen.ts dotenv_config_path=apps/myra/.env",
    "graphql-ebank:codegen": "graphql-codegen -r dotenv/config --config libs/ebanking/data-access/codegen.ts dotenv_config_path=apps/ebanking/.env",
    "graphql-neosys:codegen": "graphql-codegen -r dotenv/config --config libs/neosys/data-access/codegen.ts dotenv_config_path=apps/neosys-admin/.env",
    "graphql-migration:codegen": "graphql-codegen -r dotenv/config --config libs/migration/data-access/codegen.ts dotenv_config_path=apps/migration/.env",
    "graphql-emp:codegen": "graphql-codegen -r dotenv/config --config libs/employee-portal/data-access/codegen.ts dotenv_config_path=apps/employee-portal/.env",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
